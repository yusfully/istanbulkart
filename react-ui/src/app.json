{
  "pages": ["pages/index", "pages/about", "pages/contact"],
  "window": {
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "Kobil mini app",
    "backgroundColor": "#eeeeee",
    "backgroundTextStyle": "light"
  },
  "tabBar": {
    "backgroundColor": "#ffffff",
    "animatedItem": {
      "style": "line",
      "background": "#000000",
      "pos": "bottom",
      "height": "2px"
    },
    "style": "sharp",
    "posStyle": "atached",
    "textColor": "#BDBDBD",

    "activeTabStyle": {
      "icon": {
        "animate": true,
        "stroke": "#000000"
      },

      "textColor": "#000000",
      "opacity": "1"
    },
    "list": [
      {
        "icon": {
          "name": "card",
          "size": "24",
          "strokeWidth": "10",
          "stroke": "#BDBDBD"
        },

        "pagePath": "cards",
        "text": "Kartlarım"
      },

      {
        "icon": {
          "name": "list",
          "size": "24",
          "strokeWidth": "10",
          "stroke": "#BDBDBD"
        },
        "pagePath": "pages/announcements",
        "text": "Duyurular"
      },
      {
        "special": {
          "style": {
            "BackgroundColor": "#dedede",
            "transform": "scale(1.2)",
            "opacity": "1"
          },
          "hoverStyle": {
            "transform": "scale(1.3)"
          },
          "elements": {}
        },
        "icon": {
          "name": "folder",
          "size": "24",
          "strokeWidth": "2",
          "stroke": "#333333"
        },
        "pagePath": "pages/carousel",
        "text": "Carousel"
      },
      {
        "icon": {
          "name": "info",
          "size": "24",
          "strokeWidth": "10",
          "stroke": "#BDBDBD"
        },
        "pagePath": "pages/map",
        "text": "Hakkımızda"
      },
      {
        "icon": {
          "name": "mail",
          "size": "24",
          "strokeWidth": "10",
          "stroke": "#BDBDBD"
        },
        "pagePath": "pages/icons",
        "text": "İletişim"
      }
    ]
  }
}
